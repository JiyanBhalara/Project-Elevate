{
  "version": 2,
  "installCommand": "npm install",
  "buildCommand":   "npm run build",
  "github": {
    "enabled": false
  },
  "builds": [
    {
      "src": "/apps/api/src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "/apps/web/dist/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "/apps/api/src/main.ts",
      "methods": ["GET"]
    }
  ]
}
