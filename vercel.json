{
  "version": 2,
  "installCommand": "npm install",
  "buildCommand":   "npm run build",
  "github": {
    "enabled": false
  },
  "builds": [
    {
      "src": "/apps/api/src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "/apps/web/dist/*",
      "use": "@now/static"
    }
  ],
  "routes": [
  {
    "src": "^/api/(.*)$",
    "dest": "apps/api/dist/main.js"
  },
  {
    "handle": "filesystem"
  },
  {
    "src": "^/(.*)$",
    "dest": "/index.html"
  }
]

}
